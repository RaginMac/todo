using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class ResetAllcoins : MonoBehaviour {
	public DistributedCoins coinDistributeScript;

	// Use this for initialization
	void Start () {
		
	}
	
	// Update is called once per frame
	void Update () {
		
	}

	public void OnMouseDown()
	{
		coinDistributeScript.ResetEverything();
	}


}
